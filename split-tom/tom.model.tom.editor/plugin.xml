<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated tom -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="tom.presentation.TomModelWizardID"
            name="%_UI_TomModelWizard_label"
            class="tom.presentation.TomModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/TomModelFile.gif">
         <description>%_UI_TomModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated tom -->
      <editor
            id="tom.presentation.TomEditorID"
            name="%_UI_TomEditor_label"
            icon="icons/full/obj16/TomModelFile.gif"
            extensions="tom"
            class="tom.presentation.TomEditor"
            contributorClass="tom.presentation.TomActionBarContributor">
      </editor>
   </extension>

</plugin>
